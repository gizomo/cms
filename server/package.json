  "main": "src/index.ts",
  "scripts": {
    "create-migration": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:make --knexfile src/db/config.ts",
    "migrate": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest --knexfile src/db/config.ts",
    "create-seed": "node -r dotenv/config ./node_modules/knex/bin/cli.js seed:make --knexfile src/db/config.ts",
    "seed": "node -r dotenv/config ./node_modules/knex/bin/cli.js seed:run --knexfile src/db/config.ts",
    "build": "tsc",
    "start": "nodemon src/index.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "express-yields": "^1.1.2",
    "helpful-decorators": "^2.1.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "log4js": "^6.7.1",
    "nodemailer": "^6.8.0",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
